import { app } from "./app";
import { config } from "./config";

<% if (Backend === 'Express') { -%>
app.listen(config.http.port, () => console.log(`App listening on port ${config.http.port}!`));
<% } else if (Backend === 'Hapi') { -%>
app.start().then(() => console.log(`App listening on port ${config.http.port}!`));
<% } else { -%>
app.listen(config.http.port, () => console.log(`App listening on port ${config.http.port}!`));
<% } -%>